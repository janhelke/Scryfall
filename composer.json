{
    "name": "janhelke/scryfall",
	"homepage": "http://www.helke.dev",
	"keywords": ["scryfall", "scryfall.com", "api wrapper"],
    "description": "A wrapper around the API from Scryfall (scryfall.com)",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/dom-crawler": "*",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Janhelke\\Scryfall\\": "src",
            "Janhelke\\Scryfall\\Endpoint\\": "src/Endpoint",
            "Janhelke\\Scryfall\\Exception\\": "src/Exception",
            "Janhelke\\Scryfall\\Responses\\": "src/Responses"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jan Helke",
            "email": "jan@helke.dev"
        },
        {
            "name": "Ivo Geersen",
            "email": "ivo@ypho.nl"
        }
    ]
}
